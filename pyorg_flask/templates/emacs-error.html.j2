{# Emacs error display #}
{% extends 'rtd_base.html.j2' %}

{% set page_title = 'Emacs error - pyorg' %}

{% block content %}

    <h1>Error evaluating Elisp code</h1>

    <h4>Source</h4>

    <pre class="pre-wrap">{{ exc.src }}</pre>

    <h4>Contents of stdout:</h4>

    <pre>{{ exc.stdout.decode() }}</pre>

    <h4>Contents of stderr:</h4>

    <pre>{{ exc.stderr.decode() }}</pre>

{% endblock content %}
